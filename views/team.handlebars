<h1>Team Detail</h1>
<div class="container">
  <div class="row">
    <div class="col">
        <div class="card">
          <span class="border border-warning"></span>
          <div class="card-body">
            <img id="team-picture" src='https://d2p3bygnnzw9w3.cloudfront.net/req/201805011/tlogo/bbr/CHA.png'></img>
          </div>
        </div>
      </div>
      <div class="col">
          <div class="card">
            <span class="border border-warning"></span>
            <div class="card-body">
                <h5>Team Attribute 1</h5>
                <p id="1"></p>
                <h5>Team Attribute 2</h5>
                <p id="2"></p>
                <h5>Team Attribute 3</h5>
                <p id="3"></p>
                <h5>Team Attribute 4</h5>
                <p id="4"></p>
                <h5>Team Attribute 5</h5>
                <p id="5"></p>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="container">
    <div class="row">
    <div class="col" style="margin-top: 40px">
      <div class="card">
        <div class="card-body">
          <table>
            <tr>
              <td class="data-label"></td>
              <td class="name-label" id="name1">Team A</td>
              <td class="name-lable" id="name2">Compared Team</td>
            </tr>
            <tr>
              <td class="data-label">A</td>
              <td class="data-value" id="field-goal1"></td>
              <td class="data-value" id="field-goal2"></td>
            </tr>
            <tr>
              <td class="data-label">B</td>
              <td class="data-value" id="free-throw1"></td>
              <td class="data-value" id="free-throw2"></td>
            </tr>
            <tr>
              <td class="data-label">C</td>
              <td class="data-value" id="assists1"></td>
              <td class="data-value" id="assists2"></td>
            </tr>
            <tr>
              <td class="data-label">D</td>
              <td class="data-value" id="steals1"></td>
              <td class="data-value" id="steals2"></td>
            </tr>
            <tr>
              <td class="data-label">E</td>
              <td class="data-value" id="blocks1"></td>
              <td class="data-value" id="blocks2"></td>
            </tr>
            <tr>
              <td class="data-label">F</td>
              <td class="data-value" id="points1"></td>
              <td class="data-value" id="points2"></td>
            </tr>
            <tr>
              <td class="data-label">G</td>
              <td class="data-value" id="minutes1"></td>
              <td class="data-value" id="minutes2"></td>
            </tr>
          </table>
          <form class="form-inline my-2 my-lg-0">
            <input class="form-control mr-sm-2" type="search" placeholder="Add a Team to Compare" aria-label="Search">
            <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
